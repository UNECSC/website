{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "serve dist",
    "build": "parcel build index.html"
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": "18.20.2"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "serve": "^14.2.3"
  },
  "devDependencies": {
    "parcel": "^2.13.3",
    "@parcel/transformer-js": "^2.13.3",
    "@parcel/transformer-react-refresh-wrap": "^2.13.3"
  },
  "targets": {
    "default": {
      "distDir": "dist",
      "publicUrl": "./",
      "context": "browser",
      "outputFormat": "esmodule",
      "includeNodeModules": true,
      "sourceMap": true
    }
  }
}